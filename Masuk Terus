1\
absen = list(map(int, input().split()))
orang = 0

for i in range(len(absen)):
    if absen[i] > 0:
        orang += 1
    else:
        break
print(orang)

2/
absen = list(map(int, input().split()))
listx = []

for i in range(len(absen)):
    if absen[i] > 0:
        listx.append(absen[i])

for i in range (len(listx)):
    print(listx[i])
